{
  "name": "react-youtube-search",
  "version": "1.0.0",
  "description": "Easily search and watch YouTube videos",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently -c green,blue -n server,client \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MrWhiteNerdy/react-youtube-search.git"
  },
  "keywords": [
    "react",
    "youtube",
    "api"
  ],
  "author": {
    "name": "Joseph Davis",
    "email": "joseph.n.davis.98@gmail.com",
    "url": "https://www.josephndavis.com"
  },
